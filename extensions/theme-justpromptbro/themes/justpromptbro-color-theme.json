	"tokenColors": [
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#636366",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["string", "string.quoted"],
			"settings": {
				"foreground": "#00cc6a"
			}
		},
		{
			"scope": ["constant.numeric", "constant.language", "constant.character"],
			"settings": {
				"foreground": "#ffd60a"
			}
		},
		{
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#ff9f0a",
				"fontStyle": ""
			}
		},
		{
			"scope": ["entity.name.function", "support.function"],
			"settings": {
				"foreground": "#5ac8fa"
			}
		},
		{
			"scope": ["entity.name.class", "entity.name.type", "support.class"],
			"settings": {
				"foreground": "#bf5af2",
				"fontStyle": ""
			}
		},
		{
			"scope": ["variable", "variable.parameter"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#00cc6a"
			}
		},
		{
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#ff9f0a"
			}
		},
		{
			"scope": ["punctuation"],
			"settings": {
				"foreground": "#8e8e93"
			}
		},
		{
			"scope": ["meta.brace"],
			"settings": {
				"foreground": "#8e8e93"
			}
		},
		{
			"scope": ["support.type.property-name"],
			"settings": {
				"foreground": "#5ac8fa"
			}
		},
		{
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#ff453a"
			}
		},
		{
			"scope": ["markup.heading"],
			"settings": {
				"foreground": "#00cc6a",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["markup.bold"],
			"settings": {
				"foreground": "#ffffff",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["markup.italic"],
			"settings": {
				"foreground": "#ffffff",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["markup.inline.raw"],
			"settings": {
				"foreground": "#00cc6a"
			}
		}
	]
}
